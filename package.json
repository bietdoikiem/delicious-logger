    "url": "https://github.com/bietdoikiem"
  },
  "license": "GPL-3.0",
  "repository": "https://github.com/bietdoikiem/delicious-logger",
  "dependencies": {
    "express": "^4.17.2",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.0",
    "ts-node": "^10.4.0"
  }
}
