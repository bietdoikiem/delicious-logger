{
  "name": "delicious-logger",
  "version": "1.0.0",
  "description": "A \"Delicious\" malicious logger for Express framework to demonstrate security issues with external dependencies",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "watch": "tsc -p ./tsconfig.prod.json -w",
    "build": "tsc -p ./tsconfig.prod.json",
    "server:victim": "source .env && PORT=$VICTIM_APP_PORT env $(cat .env) ts-node --project ./tsconfig.test.json ./tests/victim/app.ts",
    "server:receiver": "source .env && PORT=$RECEIVER_APP_PORT env $(cat .env) ts-node --project ./tsconfig.test.json ./tests/receiver/app.ts"
  },
  "keywords": [],
  "author": {
    "name": "Nguyen Pham Quoc Minh",
